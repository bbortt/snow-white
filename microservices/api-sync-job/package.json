{
  "name": "api-sync-job",
  "version": "1.0.0",
  "private": true,
  "description": "Application testing dependencies of snow-white api-syn-job.",
  "author": {
    "name": "Timon Borter",
    "email": "timon.borter@gmx.ch"
  },
  "scripts": {
    "apptest:test": "run-s --print-label apptest:test:*",
    "apptest:wait": "run-p --print-label apptest:test:wait-for-minio apptest:test:wait-for-wiremock",
    "apptest:test:wait-for-minio": "wait-on --timeout 1m http-get:localhost:9090/minio/health/ready",
    "apptest:test:wait-for-wiremock": "wait-on --timeout 1m http-get:localhost:9000/__admin/health",
    "apptest:test:vitest": "vitest run --config vitest.config.ts --dir src/apptest/vitest"
  },
  "devDependencies": {
    "execa": "9.6.1",
    "npm-run-all2": "8.0.4",
    "vitest": "4.0.18",
    "wait-on": "9.0.3",
    "wiremock-captain": "4.1.3"
  },
  "packageManager": "pnpm@10.29.1",
  "engines": {
    "node": ">=22.14.0"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
